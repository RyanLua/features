{
    "rojo_at_pinned_version": {
        "image": "mcr.microsoft.com/devcontainers/base:ubuntu",
        "features": {
            "rojo": {
                "version": "7.3.0"
            }
        }
    },
    "install_aftman": {
        "image": "mcr.microsoft.com/devcontainers/base:ubuntu",
        "features": {
            "rojo": {
                "toolchainManager": "aftman"
            }
        }
    },
    "install_foreman": {
        "image": "mcr.microsoft.com/devcontainers/base:ubuntu-22.04",
        "features": {
            "rojo": {
                "toolchainManager": "foreman"
            }
        }
    },
    "install_rokit": {
        "image": "mcr.microsoft.com/devcontainers/base:ubuntu",
        "features": {
            "rojo": {
                "toolchainManager": "rokit"
            }
        }
    }
}
