{
    "id": "rojo",
    "version": "0.1.0",
    "name": "Rojo",
    "description": "Installs the provided version of Rojo, as well as Aftman and dependencies.",
    "documentationURL": "https://github.com/RyanLua/features/tree/main/src/rojo",
    "licenseURL": "https://github.com/RyanLua/features/blob/main/LICENSE",
    "keywords": ["rojo", "roblox", "aftman"],
    "options": {
        "version": {
            "type": "string",
            "proposals": [
                "latest",
                "7.4.1",
                "7.4.0",
                "7.3.0",
                "7.2.1",
                "7.1.1",
                "7.0.0",
                "6.2.0",
                "6.1.0",
                "6.0.2",
                "0.5.4",
                "0.4.13",
                "0.3.2",
                "0.2.3",
                "0.1.0"
            ],
            "default": "latest",
            "description": "Select or enter a version of Rojo to install."
        },
        "toolchain": {
            "type": "string",
            "enum": [
                "aftman",
                "foreman",
                "rokit",
                "none"
            ],
            "default": "rokit",
            "description": "Select a toolchain manager to install."
        }
    },
    "customizations": {
        "vscode": {
            "extensions": [
                "evaera.vscode-rojo"
            ]
        }
    },
    "installsAfter": [
        "ghcr.io/devcontainers/features/common-utils"
    ]
}
